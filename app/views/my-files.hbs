<div class="container">
    {{#each files}}
    <div class="card"">
        <div class="card-body d-flex justify-content-between">
          <p class="card-text">{{this.originalName}}</p>
          <a class="btn btn-primary" href="/reviews?id={{this.publicId}}" role="button">Перейти к оценкам</a>
        </div>
    </div>
    {{/each}}
</div>